<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="CustomRenderer.About">
    <ContentPage.Content>
        <AbsoluteLayout>
            <Image x:Name="BG" Source="bg1.png" VerticalOptions="CenterAndExpand" HorizontalOptions="CenterAndExpand" MinimumHeightRequest="1000" MinimumWidthRequest="1000"></Image>
            <StackLayout>
                <Image x:Name="C" Source="C1.png" VerticalOptions="CenterAndExpand" HorizontalOptions="CenterAndExpand" Margin="20,200"></Image>
            </StackLayout>
        </AbsoluteLayout>
    </ContentPage.Content>
</ContentPage>